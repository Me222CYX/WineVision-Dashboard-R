install.packages("directlabels")
library(tidyverse)
library(lubridate)
library(directlabels)
# This Dataset contains housing prices over time for different house types in the Okanagan
okHouses <- read.csv("HousingDataCREA_Adjusted.csv")
# Remove HPI columns, and "Composite", "One Floor", "Two Floor" to keep it simple
okHouses <- okHouses[-c(2:8, 10, 11)]
#Make column names more readable
colnames(okHouses) <- gsub("_.*","", colnames(okHouses))
# Tidy data
okHouses <- okHouses %>% gather(key = "HouseType", value = "AverageCost", -Date)
okHouses$HouseType <- str_replace_all(okHouses$HouseType, "Single", "Single Family")
okHouses <- na.omit(okHouses)
# Change to datetime
okHouses$Date <- my(str_to_lower(okHouses$Date))
# Housing prices over time in the Okanagan
okHouses %>% ggplot(aes(x = Date, y = AverageCost, color = HouseType)) +
geom_line() + theme_classic()+ scale_y_continuous(labels = scales::comma) +
ylab("Average Housing Cost") +
ggtitle("Average Housing Costs in the Central Okanagan") +
geom_dl(aes(label = HouseType), method = "last.points")
# Housing prices over time in the Okanagan
